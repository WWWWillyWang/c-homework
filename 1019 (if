#include "stdio.h"


int main()
{
	int g;
	while (scanf("%d", &g) != EOF) {
		if (g > 100 || g < 0) {
			printf("Error!");
		}
		else if (g >= 90) {
			printf("Letter grade: A");
		}
		else if (g >= 80) {
			printf("Letter grade: B");
		}
		else if (g >= 70) {
			printf("Letter grade: C");
		}
		else if (g >= 60) {
			printf("Letter grade: D");
		}
		else {
			printf("Letter grade: F");
		}
		printf("\n");
	}
    return 0;
}